abstract table k a
fact exclusive (table k a)

(* -------------------------------------------------------------------------- *)

(* Creation. *)

val create : [k, a] (
  capacity: int,
  hash: k -> int,
  equal: (k, k) -> bool
) -> table k a

(* -------------------------------------------------------------------------- *)

(* Clearing. *)

val clear : [k, a] table k a -> ()
val reset : [k, a] (table k a, capacity: int) -> ()

(* -------------------------------------------------------------------------- *)

(* Insertion. *)

val add : [k, a] (table k a, consumes k, consumes a) -> ()

(* -------------------------------------------------------------------------- *)

(* Removal. *)

val remove : [k, a] (t: table k a, x: k) -> ()

