mutable data xlist a = XCons { head: a; tail: xlist a } | XNil
mutable data listmaster a = Listmaster adopts xlist a

data bool = True | False

val f [a] (consumes elt: a): listmaster a =
  let x = Listmaster in
  let y =
    if True then begin
      give (XCons { head = elt; tail = XNil }: xlist a) to x;
      x
    end else begin
      give (XNil: xlist a) to x;
      x
    end
  in
  y
