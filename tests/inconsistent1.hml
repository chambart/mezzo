exclusive data t = T
data bool = True | False
data phys_equal (x :: TERM) (y :: TERM) =
| Equal { permission x = y }
| NotEqual

abstract u
fact exclusive u

val f1 (x: u, y: u): ((), ()) =
  if x == y then
    ()
  else
    ((), ())

val f2 (x: t, y: t): ((), ()) =
  if x == y then
    ()
  else
    ((), ())
