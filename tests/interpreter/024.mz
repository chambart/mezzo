data pair a b = Pair { left: a; right: b }
val make [a, b] (consumes x : a, consumes y : b) : pair a b =
  Pair { left = x; right = y }
val b () : bool =
  True
val () =
  match make (b(), b()) with
  | Pair { left = False; right = False } -> print 0
  | Pair { left = True; right = False } -> print 0
  | Pair { left = False; right = True } -> print 0
  | Pair { left = True; right = True } -> print 1
 end
