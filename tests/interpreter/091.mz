open array

val () =
  let n = 10 in
  let prev = newref 0 in
  let cur = newref 1 in
  let r : iarray int = init (Up, n,
    fun (i: int | prev @ ref int * cur @ ref int) : int =
      let tmp = !cur in
        cur := tmp + !prev;
        prev := tmp;
        tmp
  ) in
  let s = newref 0 in
  preserving (s @ ref int)
  for i = 0 to length r - 1 do
    s := !s + get (r, i);
  print !s
