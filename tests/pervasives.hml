abstract int :: TYPE
fact duplicable (int)

exclusive data xlist a =
  | XNil
  | XCons { xhead: a; xtail: xlist a }

data list a =
  | Nil
  | Cons { head: a; tail: list a }

exclusive data ref a = Ref {
  contents: a;
}

val cref [a] (x: a): ref a =
  Ref { contents = x }

val rget [a] (x: ref a): a =
  x.contents

val rset [a] (x: ref a, c: a): () =
  x.contents <- c
