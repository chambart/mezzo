abstract int :: TYPE
fact duplicable (int)

data list a =
  | Nil
  | Cons { head: a; tail: list a }

exclusive data ref a = Ref {
  contents: a;
}

val rec length [a] (x: list a): int =
  match x with
  | Nil ->
      0
  | Cons { tail = tail } ->
      1 + length tail
  end

val wrong =
  let r = Ref { contents = Nil } in
  fun [a] (x: a): () =
    r.contents <- Cons { head = a; tail = r.contents }

